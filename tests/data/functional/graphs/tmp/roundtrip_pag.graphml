<?xml version='1.0' encoding='utf-8'?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns">
  <key id="sourceEndpoint" for="edge" attr.name="sourceEndpoint" attr.type="string" />
  <key id="targetEndpoint" for="edge" attr.name="targetEndpoint" attr.type="string" />
  <graph id="G" edgedefault="directed">
    <node id="A" />
    <node id="B" />
    <node id="C" />
    <edge id="e1" source="A" target="B">
      <data key="sourceEndpoint">arrow</data>
      <data key="targetEndpoint">arrow</data>
    </edge>
    <edge id="e2" source="B" target="C">
      <data key="sourceEndpoint">circle</data>
      <data key="targetEndpoint">arrow</data>
    </edge>
  </graph>
</graphml>